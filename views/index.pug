html
    head
        title= "KenzieGram"
        link(rel="stylesheet", href="style.css", type="text/css")
    body
        h1= "KenzieGram"
        form(action="/upload" method="post" enctype="multipart/form-data")
            input(type="file" name="image")
            input(type="submit")
        each val in files
           img(src="/uploads/" + val height="150px" width="150px")
           br
        script(src="client.pug.js")